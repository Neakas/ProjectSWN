<?xml version="1.0" encoding="utf-8"?>

<Application x:Class="SWNAdmin.App" StartupUri="MainWindow.xaml"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <Color p4:Key="SWNLightBackground" xmlns:p4="http://schemas.microsoft.com/winfx/2006/xaml">#FF4B4B4B</Color>
            <Color p4:Key="SWNDarkBackground" xmlns:p4="http://schemas.microsoft.com/winfx/2006/xaml">#FF333333</Color>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary
                    Source="pack://application:,,,/Forms/EncyclopediaManager/Resources/ValueConverters.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Forms/EncyclopediaManager/Resources/Styles.xaml" />
                <ResourceDictionary
                    Source="pack://application:,,,/Forms/EncyclopediaManager/Resources/DataTemplates.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>