                 TextWrapping="Wrap" VerticalAlignment="Top" Width="120" />
        <Label x:Name="LblPlanetaxialTilt" Content="Axial Tilt:" HorizontalAlignment="Left" Margin="10,250,0,0"
               VerticalAlignment="Top" Height="20" Width="90" />
        <TextBox x:Name="TbPlanetaxialTilt" HorizontalAlignment="Left" Height="23" Margin="105,248,0,0"
                 TextWrapping="Wrap" VerticalAlignment="Top" Width="120" />
