        <Label x:Name="lblPlanetPlanetMass" Content="Planet Mass:" HorizontalAlignment="Left" Margin="10,280,0,0" VerticalAlignment="Top" Height="20" Width="90"/>
        <TextBox x:Name="tbPlanetPlanetMass" HorizontalAlignment="Left" Height="23" Margin="105,278,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
