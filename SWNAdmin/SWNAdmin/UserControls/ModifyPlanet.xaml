        <TextBox x:Name="TbPlanettecActivity" HorizontalAlignment="Left" Height="23" Margin="355,368,0,0"
                 TextWrapping="Wrap" VerticalAlignment="Top" Width="120" />
        <Label x:Name="LblPlanettideForce" Content="Tide Force" HorizontalAlignment="Left" Margin="250,400,0,0"
               VerticalAlignment="Top" Height="20" Width="90" />
        <TextBox x:Name="TbPlanettideForce" HorizontalAlignment="Left" Height="23" Margin="355,398,0,0"
                 TextWrapping="Wrap" VerticalAlignment="Top" Width="120" />
        <Label x:Name="LblPlanettideTotal" Content="Tide Total" HorizontalAlignment="Left" Margin="250,430,0,0"
               VerticalAlignment="Top" Height="20" Width="90" />
