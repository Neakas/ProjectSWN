        <Label x:Name="lblPlanetretrogradeMotion" Content="Retrograde Motion" HorizontalAlignment="Left" Margin="250,220,0,0" VerticalAlignment="Top" Height="20" Width="90"/>
        <TextBox x:Name="tbPlanetretrogradeMotion" HorizontalAlignment="Left" Height="23" Margin="355,218,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblPlanetrotPeriod" Content="Rotational Period" HorizontalAlignment="Left" Margin="250,250,0,0" VerticalAlignment="Top" Height="20" Width="90"/>
        <TextBox x:Name="tbPlanetrotPeriod" HorizontalAlignment="Left" Height="23" Margin="355,248,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblPlanetsatType" Content="Sat. Type" HorizontalAlignment="Left" Margin="250,280,0,0" VerticalAlignment="Top" Height="20" Width="90"/>
        <TextBox x:Name="tbPlanetsatType" HorizontalAlignment="Left" Height="23" Margin="355,278,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblPlanetsiderealPeriod" Content="Sidereal Period" HorizontalAlignment="Left" Margin="250,310,0,0" VerticalAlignment="Top" Height="20" Width="90"/>
        <TextBox x:Name="tbPlanetsiderealPeriod" HorizontalAlignment="Left" Height="23" Margin="355,308,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblPlanetsurfaceTemp" Content="Surface Temp." HorizontalAlignment="Left" Margin="250,340,0,0" VerticalAlignment="Top" Height="20" Width="90"/>
        <TextBox x:Name="tbPlanetsurfaceTemp" HorizontalAlignment="Left" Height="23" Margin="355,338,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblPlanettecActivity" Content="Tec. Activity" HorizontalAlignment="Left" Margin="250,370,0,0" VerticalAlignment="Top" Height="20" Width="90"/>
        <TextBox x:Name="tbPlanettecActivity" HorizontalAlignment="Left" Height="23" Margin="355,368,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblPlanettideForce" Content="Tide Force" HorizontalAlignment="Left" Margin="250,400,0,0" VerticalAlignment="Top" Height="20" Width="90"/>
        <TextBox x:Name="tbPlanettideForce" HorizontalAlignment="Left" Height="23" Margin="355,398,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblPlanettideTotal" Content="Tide Total" HorizontalAlignment="Left" Margin="250,430,0,0" VerticalAlignment="Top" Height="20" Width="90"/>
        <TextBox x:Name="tbPlanettideTotal" HorizontalAlignment="Left" Height="23" Margin="355,428,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblPlanetvolActivity" Content="Vol. Activity" HorizontalAlignment="Left" Margin="250,460,0,0" VerticalAlignment="Top" Height="20" Width="90"/>
        <TextBox x:Name="tbPlanetvolActivity" HorizontalAlignment="Left" Height="23" Margin="355,458,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblPlanetOrbitalCycle" Content="Orbital Cycle:" HorizontalAlignment="Left" Margin="250,490,0,0" VerticalAlignment="Top" Height="20" Width="90"/>
