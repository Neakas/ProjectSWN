        <Label x:Name="LblPlanetsurfaceTemp" Content="Surface Temp." HorizontalAlignment="Left" Margin="250,340,0,0"
               VerticalAlignment="Top" Height="20" Width="90" />
        <TextBox x:Name="TbPlanetsurfaceTemp" HorizontalAlignment="Left" Height="23" Margin="355,338,0,0"
                 TextWrapping="Wrap" VerticalAlignment="Top" Width="120" />
        <Label x:Name="LblPlanettecActivity" Content="Tec. Activity" HorizontalAlignment="Left" Margin="250,370,0,0"
