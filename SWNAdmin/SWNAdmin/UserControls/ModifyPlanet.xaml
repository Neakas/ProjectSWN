                 TextWrapping="Wrap" VerticalAlignment="Top" Width="120" />
        <Label x:Name="LblPlanetNightFaceMod" Content="Day Face Mod:" HorizontalAlignment="Left" Margin="250,190,0,0"
               VerticalAlignment="Top" Height="20" Width="90" />
