                 VerticalAlignment="Top" Width="120" />
        <Label x:Name="LblPlanetbbt" Content="BlackBody Temp.:" HorizontalAlignment="Left" Margin="10,520,0,0"
               VerticalAlignment="Top" Height="20" Width="90" />
        <TextBox x:Name="TbPlanetbbt" HorizontalAlignment="Left" Height="23" Margin="105,518,0,0" TextWrapping="Wrap"
                 VerticalAlignment="Top" Width="120" />
        <Label x:Name="LblPlanetdayfaceMod" Content="Day Face Mod:" HorizontalAlignment="Left" Margin="10,550,0,0"
