               VerticalAlignment="Top" Height="20" Width="90" />
        <TextBox x:Name="TbPlanethydCov" HorizontalAlignment="Left" Height="23" Margin="355,98,0,0" TextWrapping="Wrap"
                 VerticalAlignment="Top" Width="120" />
        <Label x:Name="LblPlanetisRes" Content="Is Resonant:" HorizontalAlignment="Left" Margin="250,130,0,0"
               VerticalAlignment="Top" Height="20" Width="90" />
        <TextBox x:Name="TbPlanetisRes" HorizontalAlignment="Left" Height="23" Margin="355,128,0,0" TextWrapping="Wrap"
                 VerticalAlignment="Top" Width="120" />
        <Label x:Name="LblPlanetisTideLock" Content="Tidal Locked:" HorizontalAlignment="Left" Margin="250,160,0,0"
               VerticalAlignment="Top" Height="20" Width="90" />
