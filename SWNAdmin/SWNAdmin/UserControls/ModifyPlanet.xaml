        <TextBox x:Name="tbPlanetbbt" HorizontalAlignment="Left" Height="23" Margin="105,518,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblPlanetdayfaceMod" Content="Day Face Mod:" HorizontalAlignment="Left" Margin="10,550,0,0" VerticalAlignment="Top" Height="20" Width="90"/>
