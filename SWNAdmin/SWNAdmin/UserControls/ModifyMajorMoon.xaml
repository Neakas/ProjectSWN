               VerticalAlignment="Top" Height="20" Width="90" />
        <TextBox x:Name="TbMoonId" HorizontalAlignment="Left" Height="23" Margin="105,38,0,0" TextWrapping="Wrap"
                 VerticalAlignment="Top" Width="120" IsEnabled="False" />
